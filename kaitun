    "Sub2CaptainMaui",
    "FIGHT4FRUIT",
    "EARN_FRUITS",
}
function RedeemCode(Value)
    game:GetService("ReplicatedStorage").Remotes.Redeem:InvokeServer(Value)
end
local delayBetweenRequests = 0.5
for i, v in pairs(code) do
    spawn(function()
        RedeemCode(v)
        wait(delayBetweenRequests)
    end)
end

task.wait(1)
local MyLevel = player.Data.Level.Value
if World1 then
    if MyLevel >= 1 and MyLevel <= 9 then
        Mon = "Bandit"
        LevelQuest = 1
        NameQuest = "BanditQuest1"
        NameMon = "Bandit"
        CFrameQuest = CFrame.new(1059.37195, 15.4495068, 1550.4231, 0.939700544, -0, -0.341998369, 0, 1, -0, 0.341998369, 0, 0.939700544)
        CFrameMon = CFrame.new(1045.962646484375, 27.00250816345215, 1560.8203125)
        status = "Farming Bandit"
    end
end

spawn(function()
    while task.wait(0.1) do
        pcall(function()
            local enemies = game.Workspace.Enemies:GetChildren()
            local MonsterCount = 0
            for _, enemy in ipairs(enemies) do
                if MonsterCount >= 2 then
                    break
                end
                if enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") then
                    local humanoid = enemy:FindFirstChild("Humanoid")
                    local rootPart = enemy:FindFirstChild("HumanoidRootPart")
                    if humanoid and rootPart and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        local distance = (rootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude
                        if getgenv().StartMagnet and (enemy.Name == Mon) and humanoid.Health > 0 and distance <= 350 then
                            if (enemy.Name == Mon) and CFrameMon and (rootPart.Position - CFrameMon.Position).Magnitude <= 4500 then
                                if rootPart.Parent then
                                    rootPart.CanCollide = false
                                    rootPart.Size = Vector3.new(60, 60, 60)
                                    rootPart.CFrame = CFrameMon
                                    enemy.Head.CanCollide = false
                                    local animator = humanoid:FindFirstChild("Animator")
                                    if animator then
                                        pcall(function()
                                            animator:Destroy()
                                        end)
                                    end
                                    sethiddenproperty(player, "SimulationRadius", math.huge)
                                    MonsterCount = MonsterCount + 1
                                end
                            end
                        end
                    end
                end
            end
        end)
    end
end)
