(() => {
    const isYeumoneyPage = /^https:\/\/yeumoney\.com\/[^\/]+$/.test(window.location.href);

    if (!isYeumoneyPage) {
        console.log("%c[Konmeo Hub] ‚ùå Sai web. Vui l√≤ng d√πng trang https://yeumoney.com/...", "color: red; font-weight: bold;");
        return;
    }

    console.log("%c[Konmeo Hub] üö® ƒêang b·∫≠t anti detect. Vui l√≤ng ƒë·ª£i...", "color: orange; font-weight: bold;");

    const speedMultiplier = 60;

    (() => {
        const originalDateNow = Date.now.bind(Date);
        const originalPerfNow = performance.now.bind(performance);

        const startSystem = originalDateNow();
        const startPerf = originalPerfNow();

        Date.now = () => startSystem + (originalDateNow() - startSystem) * speedMultiplier;
        performance.now = () => startPerf + (originalPerfNow() - startPerf) * speedMultiplier;

        const originalSetTimeout = window.setTimeout.bind(window);
        window.setTimeout = (fn, ms, ...args) => originalSetTimeout(fn, ms / speedMultiplier, ...args);

        const originalSetInterval = window.setInterval.bind(window);
        window.setInterval = (fn, ms, ...args) => originalSetInterval(fn, ms / speedMultiplier, ...args);
    })();

    console.log("%c[Konmeo Hub] ‚úÖ Anti detect ƒë√£ ƒë∆∞·ª£c b·∫≠t. C√≥ th·ªÉ ti·∫øp t·ª•c.", "color: #00ff00; font-weight: bold;");

    setTimeout(async () => {
        const target = document.querySelector("p.box-copy-code#TK1");
        if (!target) {
            console.log("%c[Konmeo Hub] ‚ö†Ô∏è Kh√¥ng t√¨m th·∫•y ph·∫ßn t·ª≠ ch·ª©a t√™n web.", "color: yellow; font-weight: bold;");
            return;
        }

        const keyword = target.textContent.trim();
        console.log(`%c[Konmeo Hub] üîç ƒê√£ nh·∫≠n di·ªán ƒë∆∞·ª£c web: ${keyword}, chu·∫©n b·ªã chuy·ªÉn t·ªõi web ƒë√≥ sau 5 gi√¢y.`, "color: cyan; font-weight: bold;");

        await new Promise(res => setTimeout(res, 5000));

        // T·∫°o iframe ƒë·ªÉ t√¨m k·∫øt qu·∫£ ƒë·∫ßu ti√™n
        const searchUrl = `https://www.google.com/search?q=${encodeURIComponent(keyword)}`;
        const iframe = document.createElement("iframe");
        iframe.style.display = "none";
        iframe.src = searchUrl;

        document.body.appendChild(iframe);

        iframe.onload = () => {
            try {
                const links = iframe.contentDocument.querySelectorAll('a');
                for (const link of links) {
                    const href = link.href;
                    if (href && href.startsWith("https") && !href.includes("google.com")) {
                        window.open(href, "_blank");
                        break;
                    }
                }
            } catch (e) {
                console.log("%c[Konmeo Hub] ‚ö†Ô∏è Kh√¥ng th·ªÉ l·∫•y k·∫øt qu·∫£ do h·∫°n ch·∫ø b·∫£o m·∫≠t tr√¨nh duy·ªát.", "color: orange; font-weight: bold;");
                // N·∫øu l·ªói, fallback m·ªü k·∫øt qu·∫£ t√¨m ki·∫øm
                window.open(searchUrl, "_blank");
            }
        };
    }, 1000);
})();
