  local adminId = 4882845089 -- ID c·ªßa admin c√≥ quy·ªÅn kick
local players = game:GetService("Players")

-- H√†m ki·ªÉm tra v√† x·ª≠ l√Ω l·ªánh
local function onChatted(player, message)
    if player.UserId == adminId then
        local args = string.split(message, " ")
        if args[1] == "!kick" and args[2] then
            local targetInput = args[2] -- C√≥ th·ªÉ l√† t√™n ho·∫∑c UserID
            local reason = table.concat(args, " ", 3) -- L√Ω do kick (t·ª´ t·ª´ kh√≥a th·ª© 3 tr·ªü ƒëi)
            if reason == "" then
                reason = "No reason given" -- N·∫øu kh√¥ng c√≥ l√Ω do, ƒë·∫∑t m·∫∑c ƒë·ªãnh
            end

            local targetPlayer = nil
            
            -- T√¨m ng∆∞·ªùi ch∆°i theo Username ho·∫∑c UserID
            for _, p in pairs(players:GetPlayers()) do
                if p.Name:lower() == targetInput:lower() or tostring(p.UserId) == targetInput then
                    targetPlayer = p
                    break
                end
            end

            -- N·∫øu t√¨m th·∫•y ng∆∞·ªùi ch∆°i, kick h·ªç v·ªõi l√Ω do
            if targetPlayer then
                targetPlayer:Kick("You have been kicked by an admin.\nReason: " .. reason)
                print(targetPlayer.Name .. " (UserID: " .. targetPlayer.UserId .. ") has been kicked by " .. player.Name .. " | Reason: " .. reason)
            else
                print("Player not found: " .. targetInput)
            end
        end
    end
end

-- G·∫Øn s·ª± ki·ªán khi ai ƒë√≥ nh·∫≠p l·ªánh
players.PlayerAdded:Connect(function(player)
    player.Chatted:Connect(function(message)
        onChatted(player, message)
    end)
end)

-- Ki·ªÉm tra v·ªõi ng∆∞·ªùi ch∆°i ƒëang c√≥ trong server
for _, player in pairs(players:GetPlayers()) do
    player.Chatted:Connect(function(message)
        onChatted(player, message)
    end)
end



